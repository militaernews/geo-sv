		"typescript": "5.9.3",
		"typescript-eslint": "8.46.4",
		"unplugin-icons": "22.5.0",
		"vite": "7.2.2"
	},
	"dependencies": {
		"@boxfish-studio/sveltekit-cookie-manager": "1.0.3",
		"@iconify/svelte": "5.1.0",
		"sveltekit-superforms": "2.28.1",
		"valibot": "1.1.0"
	}
}
